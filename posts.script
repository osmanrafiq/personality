INSERT INTO POSTS VALUES('enhedslisten','223040066022_10152759764711023','da','2014-10-31 12:37:22.000000','Enhedslisten \u00f8nsker god weekend med et magisk tal: Vi har lige rundet 40.000 f\u00f8lgere p\u00e5 Facebook:) \u000a\u000aDejligt, at vi er s\u00e5 mange, der \u00f8nsker et mere gr\u00f8nt, lige og retf\u00e6rdigt Danmark!')
