INSERT INTO POSTS VALUES('venstre.dk','21465928829_10152907652063830','da','2014-12-30 13:29:33.000000','Mener du ogs\u00e5, at boligjobordningen - ogs\u00e5 kaldet h\u00e5ndv\u00e6rkerfradraget - b\u00f8r bevares?')
