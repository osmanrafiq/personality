INSERT INTO POSTS VALUES('danskfolkeparti','520449347983427_1055923267769363','da','2015-02-26 10:15:49.000000','Regeringen vil have iv\u00e6rksat en unders\u00f8gelse for at konstatere, om Hizb ut-Tahrir kan lukkes, men det er jo desv\u00e6rre ikke f\u00f8rste gang, at s\u00e5dan en unders\u00f8gelse s\u00e6ttes i v\u00e6rk uden resultat. \u000aI DF har vi k\u00e6mpet i mange \u00e5r for at f\u00e5 lukket Hizb ut-Tahrir. Vi mener ikke, at det er s\u00e5 sv\u00e6rt: Luk den dog nu og luk ogs\u00e5 Grimh\u00f8jmosk\u00e9en. Enig?')
