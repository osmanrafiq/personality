INSERT INTO POSTS VALUES('Konservative','39233495912_10152675687170913','da','2015-03-09 07:27:00.000000','Alle, der har betalt for meget i boligskat, skal selvf\u00f8lgelig have hver en \u00f8re og hver en krone tilbage. Alt andet er uanst\u00e6ndigt!')
