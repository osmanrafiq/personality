INSERT INTO POSTS VALUES('sfparti','74796954245_10153304382694246','da','2015-05-17 12:44:18.000000','Ryanair viser deres sande ansigt, n\u00e5r de nu pudser dyre advokater p\u00e5 Berlingske for at undg\u00e5 negativ omtale af deres horrible ans\u00e6ttelsesvilk\u00e5r. P\u00e5 SFs landsm\u00f8de havde vi bes\u00f8g af den tidligere Ryanair stewardesse Alessandra Cocca. Og hvis man er i tvivl om Ryanairs arbejdsforhold, s\u00e5 skal man bare l\u00e6se hendes historie. Vi skal stoppe social dumping - for det er der intet socialt ved!')
