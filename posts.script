INSERT INTO POSTS VALUES('radikalevenstre','12458150929_10153345130420930','da','2015-06-08 12:10:29.000000','"Vicestatsministeren er jo stolt af det," sagde Lars L\u00f8kke i aftes p\u00e5 DR om de forbedringer, regeringen har lavet i asylsystemet i Danmark.\u000a\u000aOg ja! Vi er stolte af de 31 forbedringer af udl\u00e6ndingpolitikken, som vi har lavet. L\u00e6s dem her:\u000ahttps://radikale.dk/content/her-er-31-forbedringer-i-udl%C3%A6ndingepolitikken-som-regeringen-har-gennemf%C3%B8rt\u000a\u000a#DetHandlerOmMennensker')
