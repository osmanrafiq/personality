INSERT INTO POSTS VALUES('Konservative','39233495912_10152686736395913','da','2015-03-10 11:32:00.000000','Regeringen vil afv\u00e6bne hjemmev\u00e6rnet, s\u00e5 terrorberedskabet kan styrkes.\u000a\u000aDet giver ingen mening. Hjemmev\u00e6rnet skal selvf\u00f8lgelig have funktionsdygtige v\u00e5ben. Ellers er de ikke meget v\u00e6rd.')
