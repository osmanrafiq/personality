INSERT INTO POSTS VALUES('alternativet.dk','676061179094769_976423629058521','da','2015-06-02 19:00:36.000000','Der er fuld gang i Join The Party - Alternativets mobile valgfest - i Aalborg. G\u00e6ster: Ruth Christensen fra skifergaslejren i Dybvad, Martin B\u00f8dker fra Kemifrit DK og Flemming Lindvang fra det Hem''lige teater. Der er ogs\u00e5 musik p\u00e5 programmet: Marie Frank, Lucy Mardou og Sigurvin Sigurdsson.\u000aP\u00e5 Uffes Twitter live-streamer vi talkshowdelen via Periscope, som du kan l\u00e6se mere om her, hvis du ikke kender det: http://wiki.alternativet.dk/index.php/Sociale_medier#Periscope_-_se_livetransmissioner_igennem_din_twitter. Find streamingvideoen p\u00e5 Uffes egen Twitter-profil: https://twitter.com/uffeelbaek')
