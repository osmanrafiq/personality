INSERT INTO POSTS VALUES('enhedslisten','223040066022_10153081298471023','da','2015-03-18 09:02:08.000000','Klokken 15:30 i eftermiddag afg\u00e5r FR633 fra K\u00f8benhavns Lufthavn mod Dublin. Det er Ryan Airs f\u00f8rste flyvning fra K\u00f8benhavn, og flyet letter med personale ombord, som ikke er omfattet af nogen overenskomst. Ryan Air har n\u00e6gtet at tegne dansk overenskomst med deres medarbejdere i Danmark.\u000a\u000aVi siger nej til social dumping i luften! Del, hvis du er enig!')
