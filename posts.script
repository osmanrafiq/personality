INSERT INTO POSTS VALUES('enhedslisten','223040066022_10153079061566023','da','2015-03-17 11:47:42.000000','Pernille Skipper: En kommende folkeafstemning handler ikke om, hvorvidt Danmark skal samarbejde internationalt om kriminalitetsbek\u00e6mpelse. Den handler om igen at flytte mere magt fra den danske befolkning til EU p\u00e5 et afg\u00f8rende omr\u00e5de, der kan sv\u00e6kke vores retssikkerhed.')
