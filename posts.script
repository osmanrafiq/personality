INSERT INTO POSTS VALUES('alternativet.dk','676061179094769_981327721901445','da','2015-06-09 09:16:02.000000','"Lige nu bruger offentligt ansatte i gennemsnit en time om dagen p\u00e5 at registrere egen arbejdsindsats \u2013 det svarer til 60 mio. arbejdstimer om \u00e5ret. 60 mio. arbejdstimer, hvor i hvert fald 1/3 af dem kunne blive brugt p\u00e5 kerneopgaver som fx undervisning af vore b\u00f8rn samt pleje af vore \u00e6ldre." Uffe Elb\u00e6k forklarer her, hvad vi mener, n\u00e5r vi siger, at vi vil s\u00e6tte de offentlige institutioner fri: http://alternativet.dk/mindre-stat-mere-samfund/')
