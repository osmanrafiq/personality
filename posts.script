INSERT INTO POSTS VALUES('sfparti','74796954245_10153227024529246','da','2015-04-19 10:35:55.000000','Vi gl\u00e6der os til at byde velkommen til Poul Nyrup Rasmussen, som taler ved vores landsm\u00f8de her i dag. Han arbejder ligesom SF for en ordentlig regulering af den finansielle sektor og bek\u00e6mpelse af skattely.')
