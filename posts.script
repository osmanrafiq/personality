INSERT INTO POSTS VALUES('sfparti','74796954245_10153224936509246','da','2015-04-18 17:20:00.000000','Kim Simonsen, HK: "Vi skal have et nyt og forbedret dagpengesystem. Det fortjener danskerne!" \u000aVi er enige. Derfor foresl\u00e5r SF f\u00f8lgende: \u000a- Genoptjeningskravet skal halveres, s\u00e5 det igen er p\u00e5 seks m\u00e5neder.\u000a- Alle ledige skal have ret til tre \u00e5rs dagpenge.\u000a#sflm15')
