INSERT INTO POSTS VALUES('venstre.dk','21465928829_10152751206248830','da','2014-10-23 19:53:00.000000','Vi er enige med Kn\u00e6k Cancer: Kr\u00e6ft skal bek\u00e6mpes. Vi \u00f8nsker at lave en Kr\u00e6ftplan IV, der blandt skal forbedre kr\u00e6ftbehandlingen med 380 millioner kroner til n\u00e6ste \u00e5r. Det kan du l\u00e6se mere om her:\u000a\u000ahttp://www.venstre.dk/nyheder/1-milliard-ekstra-til-sundhed-i-2015')
